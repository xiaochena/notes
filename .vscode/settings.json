{
  "pasteImage.path": "${projectRoot}/attachments",
  "pasteImage.defaultName": "${currentFileNameWithoutExt}-Y-MM-DD-HH-mm-ss",
  "pasteImage.basePath": "${projectRoot}",
  "pasteImage.prefix": "/",
  "pasteImage.insertPattern": "![${imageFileNameWithoutExt}](${imageFilePath})",
  "pasteImage.showFilePathConfirmInputBox": false,
  "markdown.extension.print.includeVscodeStylesheets": true,
  "markdown-preview-github-styles.colorTheme": "light"
}
